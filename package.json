{
  "name": "funtime-monorepo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "turbo run db:generate && turbo run build",
    "build-web": "turbo run db:generate && turbo run build --filter=@funtime/web",
    "web-cron": "cd apps/web && bun run cron",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "start-web": "cd apps/web && pnpm run start",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:pull": "turbo run db:pull",
    "db:studio": "turbo run db:studio"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/file-saver": "^2.0.7",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "ajv": "^8.17.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.7",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "turbo": "^2.5.4",
    "typescript": "~5.8.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.30.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^19.2.7",
      "@types/react-dom": "^19.2.3"
    }
  },
  "dependencies": {
    "ioredis": "^5.7.0"
  }
}
